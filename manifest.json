{
  "name": "Youtube Scrobbler",
  "version": "0.1",
  "description": "Scrobbles songs included in youtube videos",
  "options_page": "options.html",
  "permissions": [
    "http://*.youtube.com/"
  ],
  "content_scripts": [
    {
      "matches": ["http://*.youtube.com/*"],
      "js": ["jquery-1.4.2.min.js", "md5.js", "youtube.js"]
    }
  ]
}
